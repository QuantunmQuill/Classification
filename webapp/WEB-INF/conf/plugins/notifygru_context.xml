	<!-- Transaction Manager -->
	<bean id="workflow-notifygru.transactionManager" class="fr.paris.lutece.portal.service.database.DAOUtilTransactionManager"
		p:pluginName="workflow-notifygru" />
		
	<!-- TaskInfoProvider -->
	<bean id="workflow-notifygru.notifyDirectoryTaskInfoProvider" class="fr.paris.lutece.plugins.workflow.modules.notifygru.service.NotifyGruTaskInfoProvider"
		p:taskType-ref="workflow-notifygru.taskTypeNotifyGru" />
		
	<!-- DAO -->
        
	<bean id="taskNotifyGruConfigDAO" class="fr.paris.lutece.plugins.workflow.modules.notifygru.business.TaskNotifyGruConfigDAO"/>
 	<bean id="taskNotifyGruResourceKeyDAO" class="fr.paris.lutece.plugins.workflow.modules.notifygru.business.ResourceKeyDAO"/>
 	
	<!-- Task -->
 	<bean id="workflow-notifygru.taskTypeNotifyGru" class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType"
 		p:key="taskNotifyGru"
 		p:titleI18nKey="module.workflow.notifygru.task_notify_gru_title"
 		p:beanName="workflow-notifygru.taskNotifyGru"
 		p:configBeanName="workflow-notifygru.taskNotifyGruConfig"
 		p:configRequired="true"
 		p:taskForAutomaticAction="true" />
