   	<bean id="workflow-notifygru.notifyDirectoryTaskComponent" class="fr.paris.lutece.plugins.workflow.modules.notifygru.web.NotifyGruTaskComponent"
   		p:taskType-ref="workflow-notifygru.taskTypeNotifyGru"
   		p:taskConfigService-ref="workflow-notifygru.taskNotifyGruConfigService" />
   	
   	<!-- Service -->
   	<bean id="workflow-notifygru.notifyGruService" class="fr.paris.lutece.plugins.workflow.modules.notifygru.service.NotifyGruService" />
   	<bean id="workflow-notifygru.taskNotifyGruConfigService" class="fr.paris.lutece.plugins.workflow.modules.notifygru.service.TaskNotifyGruConfigService"
   		p:taskConfigDAO-ref="taskNotifyGruConfigDAO" />
   	<bean id="workflow-notifygru.resourceKeyService" class="fr.paris.lutece.plugins.workflow.modules.notifygru.service.ResourceKeyService" />
	
	
	<!-- UserDashboard 
	<bean id="mydashboard-gru.mydashboardBillsComponent" class="fr.paris.lutece.plugins.mydashboard.modules.gru.web.MyDashboardBillsComponent" />
    
	-->
        
